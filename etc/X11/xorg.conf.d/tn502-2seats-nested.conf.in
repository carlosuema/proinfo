Section "Device"
    MatchSeat "__x-daemon@TN502_DISPLAY@__"
    Identifier "videocard_tn502"
    BusID "PCI:@TN502_ADDRESS@"
    Driver "siliconmotion"
    Option "PanelSize" "1360x768"
EndSection

Section "Screen"
    MatchSeat "__x-daemon@TN502_DISPLAY@__"
    Identifier "screen_tn502"
    Device "videocard_tn502"
    DefaultDepth 16
EndSection
